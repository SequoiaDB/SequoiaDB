		<Default>/opt/sequoiadb/database/standalone</Default>
		<Valid></Valid>
		<Display>text box</Display>
		<Edit>true</Edit>
		<Desc>数据存储路径</Desc>
		<Level>0</Level>
		<DynamicEdit>false</DynamicEdit>
		<BatchEdit>false</BatchEdit>
	</FILLER>
	<FILLER>
		<Name>svcname</Name>
