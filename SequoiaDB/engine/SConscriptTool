		 sdbSEAdptFiles,
		 LIBDEPS=["pd","oss","msg","util","bson","pmd","pmdMT","net","rtnJob","rtn","ixm","cls","mon"],
		 _LIBDEPS='$_LIBDEPS_OBJS' )

