    Import("fuse_lib")
    sequoiafs = toolEnv.Program("sequoiafs",
        sequoiaFSFiles,
        LIBDEPS=["bson", "oss", "pd", "util", "pmd", "client", "clientcpp", "mon", fuse_lib],
        _LIBDEPS='$_LIBDEPS_OBJS' )

