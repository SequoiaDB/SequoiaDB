         LIBDEPS=["pd","oss","pmd","bson","util","mon"],
         _LIBDEPS='$_LIBDEPS_OBJS' )

sdbstop = toolEnv.Program("sdbstop", sdbStopMain,
         LIBDEPS=["pd","oss","bson","util","mon"],
         _LIBDEPS='$_LIBDEPS_OBJS' )

