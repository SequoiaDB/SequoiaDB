# Build 8 flavors of libbid.a in 32-bit data mode on HPUX using the HP compiler
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=0 GLOBAL_RND=0 GLOBAL_FLAGS=0 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_000libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=0 GLOBAL_RND=0 GLOBAL_FLAGS=1 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_001libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=0 GLOBAL_RND=1 GLOBAL_FLAGS=0 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_010libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=0 GLOBAL_RND=1 GLOBAL_FLAGS=1 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_011libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=1 GLOBAL_RND=0 GLOBAL_FLAGS=0 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_100libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=1 GLOBAL_RND=0 GLOBAL_FLAGS=1 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_101libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=1 GLOBAL_RND=1 GLOBAL_FLAGS=0 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_110libbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD32=1 BID_BIG_ENDIAN=true CALL_BY_REF=1 GLOBAL_RND=1 GLOBAL_FLAGS=1 UNCHANGED_BINARY_FLAGS=0;
mv libbid.a cc32_111libbid.a;
gmake clean ;



