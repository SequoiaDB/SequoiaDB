mv libbid.a cc64_011blibbid.a;
gmake clean ;
gmake  CC=cc HPUX=1 DD64=1 BID_BIG_ENDIAN=true CALL_BY_REF=1 GLOBAL_RND=0 GLOBAL_FLAGS=0 UNCHANGED_BINARY_FLAGS=1;
mv libbid.a cc64_100blibbid.a;
