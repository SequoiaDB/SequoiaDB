gmake  CC=cc HPUX=1 DD64=1 BID_BIG_ENDIAN=true CALL_BY_REF=1 GLOBAL_RND=1 GLOBAL_FLAGS=1 UNCHANGED_BINARY_FLAGS=1;
mv libbid.a cc64_111blibbid.a;
gmake clean ;
