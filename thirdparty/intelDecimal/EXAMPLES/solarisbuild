echo ""
echo ""
echo "***************** RUNNING EXAMPLE FOR gcc 000 **************************"
echo ""
echo ""
cp main.c_000 main.c
cp decimal.h_000 decimal.h
cp ../LIBRARY/gcc000libbid.a ../LIBRARY/libbid.a
gcc main.c -DBID_BIG_ENDIAN ../LIBRARY/libbid.a
./a.out
rm a.out
echo ""
echo ""
