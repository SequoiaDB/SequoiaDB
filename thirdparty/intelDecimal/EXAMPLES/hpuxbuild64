echo "";
echo "";
echo "***************** RUNNING EXAMPLE FOR cc 000 (64-bit data mode) **************************";
echo "";
echo "";
cp main.c_000 main.c;
cp decimal.h_000 decimal.h;
cp ../LIBRARY/cc64_000libbid.a ../LIBRARY/libbid.a;
/opt/ansic/bin/cc +DD64 main.c -DHPUX_OS -DBID_BIG_ENDIAN ../LIBRARY/libbid.a;
./a.out;
rm a.out;
