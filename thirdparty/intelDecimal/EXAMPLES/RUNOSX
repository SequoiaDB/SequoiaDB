./linuxbuild > linuxout
# grep PASS linuxout
cat linuxout
grep FAIL linuxout
rm linuxout main.c decimal.h
echo "END BUILDING AND RUNNING EXAMPLES IN LINUX..."
echo "(THE TESTS PASSED IF THE WORD 'FAIL' WAS NOT PRINTED ABOVE)"

