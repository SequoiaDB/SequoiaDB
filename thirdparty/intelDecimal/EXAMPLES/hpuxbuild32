echo "";
echo "";
echo "***************** RUNNING EXAMPLE FOR cc 000 (32-bit data mode) **************************";
echo "";
echo "";
cp main.c_000 main.c;
cp decimal.h_000 decimal.h;
cp ../LIBRARY/cc32_000libbid.a ../LIBRARY/libbid.a;
/opt/ansic/bin/cc +DD32 main.c -DHPUX_OS -DBID_BIG_ENDIAN ../LIBRARY/libbid.a;
./a.out;
rm a.out;
